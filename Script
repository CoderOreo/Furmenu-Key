local function QFZLEP_fake_script() -- Speed.LocalScript 
	local script = Instance.new('LocalScript', Speed)

	local Speed = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		Speed = Speed + 5
	end)
	
	script.Parent.Parent.RSpeed.MouseButton1Click:Connect(function()
		Speed = 0
	end)
	
	local hb = game:GetService("RunService").Heartbeat
	
	game:GetService("RunService").Heartbeat:Connect(function()
		local delta = hb:Wait()
		if game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude > 0 then
				game.Players.LocalPlayer.Character:TranslateBy(game.Players.LocalPlayer.Character.Humanoid.MoveDirection * tonumber(Speed) * delta * 10)
		end
	end)
end
coroutine.wrap(QFZLEP_fake_script)()
local function TNWOBO_fake_script() -- Depixelize.LocalScript 
	local script = Instance.new('LocalScript', Depixelize)

	script.Parent.MouseButton1Click:Connect(function()
		local Saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
		local vim = game:GetService("VirtualInputManager")
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(9e9,9e9 + 9e9 + 9e9 + 9e9,9e9)
		--game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,39000,0)
		wait(0.05)
		vim:SendKeyEvent(true, Enum.KeyCode.G, false, game)
		wait(0.05)
		vim:SendKeyEvent(false, Enum.KeyCode.G, false, game)
		wait(0.01)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Saved
		game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
	end)
end
