-- Scripts:

local function KPTEKH_fake_script() -- ImageLabel.Rainbow 
	local script = Instance.new('Script', ImageLabel)

	local text = script.Parent
	local add = 10
	--wait(1)
	local k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(k/255,0/255,0/255)
	k = k + add
	wait()
	end
	while true do
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(255/255,k/255,0/255)
	k = k + add
	wait()
	end
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(255/255 - k/255,255/255,0/255)
	k = k + add
	wait()
	end
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(0/255,255/255,k/255)
	k = k + add
	wait()
	end
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(0/255,255/255 - k/255,255/255)
	k = k + add
	wait()
	end
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(k/255,0/255,255/255)
	k = k + add
	wait()
	end
	k = 0.9
	while k <= 255 do
	text.ImageColor3 = Color3.new(255/255,0/255,255/255 - k/255)
	k = k + add
	wait()
	end
	while k <= 255 do
	text.ImageColor3 = Color3.new(255/255 - k/255,0/255,0/255)
	k = k + add
	wait()
	end
	end
end
coroutine.wrap(KPTEKH_fake_script)()
local function BHGR_fake_script() -- UIGradient.Script 
	local script = Instance.new('Script', UIGradient)

	while task.wait() do
		if script.Parent.Rotation > 179.9 then
			script.Parent.Rotation = 0
		else
			script.Parent.Rotation = script.Parent.Rotation + 1.5
		end
	end
end
coroutine.wrap(BHGR_fake_script)()
local function QYCFHUY_fake_script() -- Intro.Script 
	local script = Instance.new('Script', Intro)

	wait(3)
	local TweenService = game:GetService("TweenService")
	local PartTween = TweenService:Create(script.Parent.Bar.Percent, TweenInfo.new(0.4), {Size = UDim2.new({0.5, 0},{1, 0})})
	PartTween:Play()
	script.Parent.Progress.Text = "Setting Up UI..."
	wait(0.4)
	script.Parent.Progress.Text = "Checking Key..."
	wait(0.5)
	local nokey = false
	local worked,errormsg = pcall(function()
		local Text = tostring(readfile("furmenukeysave.txt"))
		local ActualKey = tostring(game:HttpGet("https://raw.githubusercontent.com/CoderOreo/Furmenu-Panel/main/key.txt"))
		if Text == ActualKey then
			--print("YES")
		else
			--print("NO")
			nokey = true
		end
	end)
	
	if worked == true then
		if nokey == false then
			if game.GameId == 1008451066 then
				script.Parent.Visible = false
				script.Parent.Parent.MainDaHood.Visible = true
			end
		end
	else
		script.Parent.Visible = false
		script.Parent.Parent.Key.Visible = true
	end
end
coroutine.wrap(QYCFHUY_fake_script)()
local function ONEBIZP_fake_script() -- Enter.LocalScript 
	local script = Instance.new('LocalScript', Enter)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.Key.Text == tostring(game:HttpGet("https://raw.githubusercontent.com/CoderOreo/Furmenu-Panel/main/key.txt")) then
			script.Parent.Parent.Parent.Parent.Visible = false
			writefile("furmenukeysave.txt",tostring(game:HttpGet("https://raw.githubusercontent.com/CoderOreo/Furmenu-Panel/main/key.txt")))
			if game.GameId == 1008451066 then
				script.Parent.Parent.Parent.Parent.Parent.MainDaHood.Visible = true
			end
			
		else
			script.Parent.Parent.Error.Visible = true
		end
	end)
end
coroutine.wrap(ONEBIZP_fake_script)()
local function TUSYB_fake_script() -- Key.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', Key)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(TUSYB_fake_script)()
