	script.Parent.MouseEnter:Connect(function()
		local TweenService = game:GetService("TweenService")
		local PartTween = TweenService:Create(script.Parent, TweenInfo.new(0.15), {BackgroundColor3 = Color3.new(0.972549, 0.129412, 0.129412)})
		PartTween:Play()
		local TweenService = game:GetService("TweenService")
		local PartTween = TweenService:Create(script.Parent, TweenInfo.new(0.15), {TextColor3 = Color3.new(1,1,1)})
		PartTween:Play()
	end)
	
	script.Parent.MouseLeave:Connect(function()
		local TweenService = game:GetService("TweenService")
		local PartTween = TweenService:Create(script.Parent, TweenInfo.new(0.15), {BackgroundColor3 = Color3.new(1,1,1)})
		PartTween:Play()
		local TweenService = game:GetService("TweenService")
